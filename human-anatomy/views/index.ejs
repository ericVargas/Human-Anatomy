<% include ./partials/boilerplate %>
    <div id="content">
        <div class="inner">

            <article class="box post post-excerpt">
                <header>
                    <h2><%= title %></h2>
                </header>
                <ul>
                    <% data.forEach(function(d) { %>
                        <li data-id=<%=d.id%>>
                            <img src=<%=d.image %>>
                            <br>
                            <p><b><%= d.description %></b></p>
                        </li>
                        <br><br><br>
                    <% }) %>
                </ul>
            </article>

        </div>
    </div>

